    "start:prod": "export NODE_ENV=master && node ./node_modules/@babel/node/bin/babel-node.js server.js",
    "build": "export NODE_ENV=qa && node ./node_modules/webpack/bin/webpack.js -d --watch",
    "build:prod": "export NODE_ENV=staging && node ./node_modules/webpack/bin/webpack.js -p"
